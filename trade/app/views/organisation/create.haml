%form(action="/organisation/create" enctype="multipart/form-data" method="post")
  %label Name:
  %br
  %input(type="text" name="name")
  %br
  %label Description:
  %br
  %textarea(name="description" cols=30 rows=10)
  %br
  %label Avatar:
  %br
  %input(type="file" name="avatar" size=50 maxlength="=100000" accept="image/*")
  %br
  %input(type="submit" id="Launch!" value="founding")